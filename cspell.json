{
    "version": "0.2",
    "language": "en",
    "allowCompoundWords": true,
    "numSuggestions": 5,
    "ignorePaths": [
        "node_modules", // ignores anything w/in dir == "**/node_modules" | "**/node_modules/**"
        "vscode-extension",
        ".git",
        "**/*.dll" // Ignore the .git dir, all .dll & .py files
    ],
    "suggestionsTimeout": 250,
    "caseSensitive": false,
    "maxDuplicateProblems": 2,
    "useGitignore": true,
    "enableFiletypes": [
        "!env",
        "!py",
        "!ipynb"
    ],
    "ignoreRegExpList": [
        "/^[A-Z][a-z0-9_-]{3,19}$/",
        "/^[^a-z]*$/"
    ],
    /*
        ["ignoreRegExpList": ["[A-F]{3,}", "/^[A-Z][a-z0-9_-]{3,19}$/", "/0x[0-9A-F]+/g", "\b[A-Z]{3,}\b", "(?:(?<!^)[A-Z]\b|(?<!^[A-Z0-9 ]*)\b[A-Z0-9]+\b(?![A-Z0-9 ]$))", "/[^\s]{40,}/"],]
    */
    "cache": {},
    "dictionaryDefinitions": [],
    "dictionaries": [],
    "words": [
        "ACPI",
        "APIPA",
        "Aplus",
        "Bahim",
        "bootable",
        "CDMA",
        "chipsets",
        "CMOS",
        "connectionless",
        "DIMM",
        "Fileshare",
        "fuser",
        "Gbps",
        "gpedit",
        "Hotspot",
        "ICANN",
        "inkjet",
        "ipynb",
        "Kbps",
        "misfeeds",
        "mobo",
        "mobos",
        "Molex",
        "netman",
        "Northbridge",
        "OLED",
        "SATA",
        "soundcards",
        "Southbridge",
        "UEFI",
        "URLLC",
        "venv",
        "VM's",
        "WLAN",
        "WPAN"
    ]
}
